      "prod": "npm run clean && vite build"
  },
  "scripts_old": {
    "build": "tsc && vite build --mode=dev"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
