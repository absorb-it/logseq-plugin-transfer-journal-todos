    "eslint": "^8.23.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.4",
    "logseq-l10n": "^0.2.0"
  },
  "dependencies": {
      "@logseq/libs": "^0.0.17",
      "date-fns": "^3.6.0",
      "date-holidays": "^3.23.12",
      "logseq-l10n": "^0.2.0"
  }
